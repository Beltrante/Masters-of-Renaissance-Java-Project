<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <ImageView fitHeight="800.0" fitWidth="723.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../images/loginImage.jpg" />
   </ImageView>
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <Label alignment="CENTER" layoutX="134.0" layoutY="174.0" prefHeight="57.0" prefWidth="331.0" text="Choose Numbers of Players" textAlignment="CENTER" textFill="#eeff38">
         <font>
            <Font name="Arial Rounded MT Bold" size="24.0" />
         </font>
      </Label>
      <ComboBox fx:id="numberSelector" layoutX="134.0" layoutY="238.0" prefHeight="25.0" prefWidth="94.0" />
      <Button fx:id="done" layoutX="261.0" layoutY="238.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="77.0" text="Done" />
      <TextArea fx:id="messageBox" blendMode="MULTIPLY" editable="false" layoutX="119.0" layoutY="365.0" prefHeight="164.0" prefWidth="364.0" />
   </AnchorPane>
</StackPane>
