<?import javafx.scene.control.TextField?>
<StackPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="800.0" fitWidth="723.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/loginImage.jpg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label alignment="CENTER" layoutX="235.0" layoutY="174.0" prefHeight="57.0" prefWidth="331.0" text="Choose Numbers of Players" textAlignment="CENTER" textFill="#030303">
               <font>
                  <Font name="Arial Rounded MT Bold" size="24.0" />
               </font>
            </Label>
            <ComboBox fx:id="numberSelector" layoutX="277.0" layoutY="238.0" prefHeight="25.0" prefWidth="94.0" />
            <Button fx:id="done" layoutX="415.0" layoutY="238.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="77.0" text="Done" />
            <TextField fx:id="messageBox" alignment="CENTER" editable="false" layoutX="439.0" layoutY="37.0" prefHeight="9.0" prefWidth="221.0" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
